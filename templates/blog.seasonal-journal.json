        },
        "c05766e0-6cfc-47df-9a98-d72cdbd748b0": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/DSC07487.jpg",
            "text-position": "bottom-left",
            "text-width": "medium",
            "overline": "",
            "title": "Skincare",
            "description": "<p>Indulgent baths, face masks, and alot of moisturizing. Our skin tends to lean dryer during the cold months, so extra attention will keep it feeling nourished. Lots of Renaissance Body Oil for sure!<\/p>",
            "link-url": "\/",
            "link-text": ""
          }
        },
